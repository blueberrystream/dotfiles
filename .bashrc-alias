alias ls='ls --color=auto'
alias ll='ls -lAp --color=auto'
alias lld='ll | grep ^d'
alias lll='ll | grep ^l'
alias llf='ll | grep ^-'
alias cp='cp -i'
alias mv='mv -i'
alias rm='rm -i'
alias cd='cdls'
alias cd..='cd ..'
alias vi=vim
alias view='vim -R'
alias psgrep='ps aux | grep'
alias reload-bash='source $HOME/.bash_profile'
